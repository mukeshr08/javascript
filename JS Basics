//Dynamic languge a=25 not use in int a=25
/**
 * primitive data type
 Number eg:23,45
 Boolean eg: true,false
 Null 
 Undefined eg: int a
 symbols E6

 Reference type
 Array
 object literals
 function
 date 
 */

 var a=25;
 var b=15.3
 console.log(typeof a)
 console.log(typeof b)
 var fname= "mukesh"
 console.log(typeof fname)
 var isNumber=true;
 console.log(typeof isNumber)
 var c=null
 console.log(typeof c)
 var d;
 console.log(d)

//  symbols - unique value used in objects
const e1=Symbol()
console.log(e1)
const e2=Symbol()
console.log(e2)
console.log(e1==e2)

// Reference type
var courses=['c','c++','python']
console.log(typeof courses)
var student={'name':"mukesh","age":18}
console.log(typeof student)
var d=new Date();
console.log( d)

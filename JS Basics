//type conversion 
//all data types into string type
let a=25;
console.log(a,typeof a);
a=String(a);
//in string S is capital
console.log(a,typeof a);

let b=25.3;
console.log(b,typeof b);
b=String(b);
console.log(b,typeof b)

let c=true;
console.log(c,typeof c);
c=String(c);
console.log(c,typeof c)


let f=new Date()
console.log(f,typeof f)
f=String(f);
console.log(f,typeof f)

let j=[1,2,3,4,5]
console.log(j,typeof j)
j=String(j);
console.log(j,typeof j)

let i=25;
console.log(i,typeof i);
i=i.toString();
//in string S is capital
console.log(i,typeof i);

//string to number
e="132"
console.log(e,typeof e)
e=Number(e);
console.log(e,typeof e)

//boolean to number
f=true
console.log(f,typeof f);
f=Number(f)
console.log(f,typeof f)

//array to number is NaN
k=[1,3,4,5,5]
console.log(k,typeof k);
k=Number(k)
console.log(k,typeof k);

//string to number  is NaN
l="mukesh"
console.log(l,typeof l);
l=Number(l)
console.log(l,typeof l);

m="mukesh"
console.log(m,typeof m);
m=parseInt(m)
console.log(m,typeof m);

//string with number into number
m="1324"
console.log(m,typeof m);
m=parseInt(m)
console.log(m,typeof m);

// string with float into number
m="13.2354"
console.log(m,typeof m);
m=parseInt(m)
console.log(m,typeof m);

//string with float into floating number
m="12.3432"
console.log(m,typeof m);
m=parseFloat(m)
console.log(m,typeof m);








